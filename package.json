{
  "name": "me-api",
  "version": "1.0.0",
  "description": "API for goudie.dev resume",
  "repository": {
    "type": "github",
    "url": "https://github.com/dgoudie/me-api"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "tsc-watch --onSuccess \"cross-env NODE_PATH=./dist node -r dotenv/config dist/app --env=dev\"",
    "prod": "tsc && cross-env NODE_PATH=./dist node dist/app --env=prod"
  },
  "dependencies": {
    "@types/lodash": "^4.14.168",
    "@types/node": "^12.19.15",
    "@types/webrtc": "0.0.26",
    "cors": "^2.8.5",
    "cross-env": "^5.2.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.9.0",
    "graphql": "^15.5.0",
    "graphql-fields": "^2.0.3",
    "lodash": "^4.17.20",
    "log4js": "^4.5.1",
    "mongodb": "^3.6.3",
    "typescript": "^3.9.7"
  },
  "devDependencies": {
    "@stan/me-types": "^1.0.1",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/graphql-fields": "^1.3.3",
    "@types/mongodb": "^3.6.3",
    "tsc-watch": "^2.4.0"
  }
}
